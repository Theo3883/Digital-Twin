# ── Digital Twin — Doctor Portal ──────────────────────────────────────────────
# Copy this file to .env.local and fill in the values.

# .NET Web API base URL (no trailing slash)
NEXT_PUBLIC_API_URL=http://localhost:5003

# Google OAuth 2.0 credentials (from Google Cloud Console)
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# NextAuth.js
AUTH_SECRET=openssl-rand-base64-32-output-here
AUTH_TRUST_HOST=true
NEXTAUTH_URL=http://localhost:3000

# Doctor registration secret (must match the Web API's Doctor:RegistrationSecret)
NEXT_PUBLIC_DOCTOR_SECRET_REQUIRED=true
